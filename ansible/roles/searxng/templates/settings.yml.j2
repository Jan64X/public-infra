# SearXNG settings

use_default_settings: true

general:
  debug: false
  instance_name: "NebulaXNG"
  base_url: "{{ searxng_base_url | default('') }}"

search:
  safe_search: 2
  autocomplete: 'duckduckgo'
  default_lang: "en"
  timeout: 10.0

server:
  secret_key: "{{ searxng_secret_key }}"
  limiter: false
  image_proxy: true
  port: 80
  bind_address: "0.0.0.0"

ui:
  static_use_hash: true
  default_theme: simple

enabled_plugins:
  - 'Hash plugin'
  - 'Self Informations'
  - 'Tracker URL remover'

engines: 